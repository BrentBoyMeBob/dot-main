{
    "telemetry.telemetryLevel": "off",
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.preferredLightColorTheme": "GitHub Light",
    "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
    "workbench.iconTheme": "vs-minimal",
    "workbench.startupEditor": "none",
    "workbench.sideBar.location": "right",
    "apc.activityBar": {
        "position": "bottom",
        "size": 32
    },
    "editor.fontFamily": "Liga SFMono Nerd Font, SF Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.accessibilitySupport": "off",
    "vim.highlightedyank.enable": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["d"],
            "after": ["\"", "_", "d"]
        },
        {
            "before": ["s"],
            "commands": ["leap.findForward"]
        },
        {
            "before": ["S"],
            "commands": ["leap.findBackward"]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<esc>"]
        }
    ],
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [80],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "files.associations": {
        "*.scm": "lisp"
    },
    "jupyter.askForKernelRestart": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "ollama-autocoder.model": "phi",
    "terminal.integrated.env.osx": {},
    "git-assistant.checkConfigVariables": "disabled",
    "interactiveWindow.executeWithShiftEnter": true
}

